<Application x:Class="ALauncher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ALauncher"
             Startup="OnStartup">
    <Application.Resources>
        <ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
            <ResourceDictionary.MergedDictionaries> 
                <local:SharedResourceDictionary Source="Styles/Styles.xaml" />
                <local:SharedResourceDictionary Source="Styles/TrayResources.xaml" />
                <ResourceDictionary>
                    <FontFamily x:Key="Gruppo">.\Fonts\#Gruppo</FontFamily>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
